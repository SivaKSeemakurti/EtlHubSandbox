{
	"name": "df_SFTP_Pull_Tririga",
	"properties": {
		"folder": {
			"name": "ETLHub"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "IWTRIRIGABuildingQuery",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "IWTRIRIGABuildingQueryCSV",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          LEASE_NUMBER as string,",
				"          BLDG_ID as string,",
				"          SUBTENANT as string,",
				"          SUBLEASE_NUMBER as string,",
				"          SUBLEASE_START_DATE as string,",
				"          SUBLEASE_END_DATE as string,",
				"          SUBLET_LOC_MEAS_QTY as string,",
				"          SUBLET_RNTBL_SQ_FT as string,",
				"          SUBLET_INCOME_USD_AMT as string,",
				"          SUBLET_INCOME_LCL_AMT as string,",
				"          LOCAL_CURRENCY as string,",
				"          CAMPUS_ID as string,",
				"          EFFECTIVE_DATE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: ['mv /extract/Tririga/part-merged.csv /extract/Tririga/IWTRIRIGABuildingQuery.csv'],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     partitionBy('hash', 1)) ~> sink1"
			]
		}
	}
}